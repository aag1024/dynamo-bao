{
  "source": {
    "include": ["./"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "include_css": ["./jsdoc.css"],
      "homepageTitle": "DynamoBao",
      "title": "DynamoBao",
      "sections": ["Namespaces", "Classes"]
    },
    "readme": "./README.md"
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
